egories.entities) {\n        const categoriesArray = [];\n        for (const categoryId of categoriesIds) {\n            for (const category of state.categories.entities) {\n                if (category._id === categoryId) {\n                    categoriesArray.push(category);\n                    break;\n                }\n            }\n        }\n        return categoriesArray;\n    }\n    return [];\n};\n\nexport default categoriesReducer;\n","/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/services/categories.service.js",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/orderCard.jsx",["312","313","314","315"],[],"import React, {useEffect, useState} from \"react\";\nimport { Link } from \"react-router-dom\";\nimport {useCart} from \"../../hooks/useCart\";\nimport OrderItem from \"./orderItem\";\n\nconst OrderCard = ( {data, index} ) => {\n    let totalPrice = 0\n\n    data.map((item) => (\n        totalPrice = totalPrice + (item.counter * item.item.price)\n    ))\n\n    return (\n        <>\n            <div className=\"orders-list\">\n                <p>Order №{index+1}</p>\n                {data.map((item) => (\n                    <OrderItem data={item}/>\n                ))}\n                <div className=\"orders-info\">\n                    <p>Totally: {totalPrice}₽</p>\n                    <p>12/12/12</p>\n                </div>\n\n            </div>\n        </>\n    );\n};\n\nexport default OrderCard;\n","/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/orderItem.jsx",["316","317","318","319","320","321"],[],"import React, {useEffect, useState} from \"react\";\nimport { Link } from \"react-router-dom\";\nimport {useCart} from \"../../hooks/useCart\";\n\nconst OrderItem = ( {data} ) => {\n    const { incrementItem, decrementItem, removeCartItem} = useCart()\n    const 