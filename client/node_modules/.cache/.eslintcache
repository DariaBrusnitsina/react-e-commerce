mming/Projects/react-e-commerce/client/src/components/ProfileLayout/profilePage.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/ProfileLayout/profileInfo.jsx",["353","354"],[],"import React, {useState} from \"react\";\nimport {Link, Navigate, NavLink, useNavigate, useParams} from \"react-router-dom\";\nimport ProfileOrderCard from \"./profileOrderCard\";\nimport ProfileOrdersListModal from \"./profileOrdersListModal\";\nimport {useSelector} from \"react-redux\";\nimport {getCurrentUserData, getDataStatus, getUsersLoadingStatus} from \"../../store/users\";\n\nconst CssClasses = {\n    PROFILE: \"profile\",\n    INFO: \"profile__info\",\n    SALE: \"profile__sale\",\n    CONTACTS: \"profile__contacts\",\n    TITLE: \"profile__title\",\n    SUBTITLE: \"profile__subtitle\",\n    FORM: \"profile__form-element\",\n    LIST: \"orders-list\",\n    REDIRECT_BTN: \"profile__btn\"\n}\n\nconst ProfileInfo = () => {\n    const user = useSelector(getCurrentUserData());\n    const navigate = useNavigate();\n    const currentUser = useSelector(getCurrentUserData())\n    const status = useSelector(getUsersLoadingStatus())\n    const statusData = useSelector(getDataStatus())\n\n    const [modalIsOpen, setIsOpen] = useState(false);\n    let ordersArray = user ? [...user.orders] : []\n    if (ordersArray.length > 3 ) {\n        ordersArray = ordersArray.slice(0,3)\n    }\n\n    if (!currentUser && !statusData && !status ) {\n        return <Navigate to=\"/\" />\n    }\n\n    function handleOpenModal() {\n        setIsOpen(true);\n    }\n\n    function handleCloseModal() {\n        setIsOpen(false);\n\n    }\n\n    if (user) {\n        return (\n            <div className={CssClasses.PROFILE}>\n                <div className=\"container\">\n\n                    <div className={CssClasses.INFO}>\n                        <div>\n                            <h1>{user.name + \" \" + user.surname}</h1>\n                            <h2 className={CssClasses.TITLE}>Your sale: {user.sale}%</h2>\n                            <div className={CssClasses.SALE}>\n                                <p>For every 1000 rubles in the order, you get +1% to the cumulative discount for the next orders. The maximum discount under the accumulative program is 15%.</p>\n                            </div>\n                        </div>\n\n                        <div className={CssClasses.CONTACTS}>\n                            <h3 className={CssClasses.SUBTITLE}>Contact information</h3>\n                            <ul>\n                                <li className={CssClasses.FORM}><span>Email:</span> {user.email}</li>\n                                <li className={CssClasses.FORM}><span>Phone:</span> {user.phone}</li>\n                                <li className={CssClasses.FORM}><span>Address:</span> {user.address ? user.address : <NavLink to={\"edit\"}>add</NavLink>}</li>\n                            </ul>\n\n                            <NavLink className={CssClasses.REDIRECT_BTN} to={'edit'}>Edit contact information</NavLink>\n                        </div>\n                    </div>\n\n                    <div>\n                        <div>\n                            <h2 className={CssClasses.TITLE}>Your last orders</h2>\n                            { user.orders && user.orders.length !== undefined\n                                ? <div>\n                                    <ul className={CssClasses.LIST}>\n                                        {ordersArray.reverse().map((data) => (\n                                            <li><ProfileOrderCard data={data}/></li>\n                                        ))}\n                                        <li>{user.orders.length > 3 ? <button className={CssClasses.REDIRECT_BTN} onClick={() => handleOpenModal()}>All orders</button> : \"\"}</li>\n                                    </ul>\n\n                                </div>\n                                : <p>You have no orders! Start <Link to=\"/shop\">shopping</Link> now.</p>\n                            }\n                        </div>\n                    </div>\n                </div>\n                <ProfileOrdersListModal orders={user.orders} closeModal={handleCloseModal} modalIsOpen={modalIsOpen}/>\n            </div>\n\n        );\n    } else {\n        return <h1>Loading</h1>;\n    }\n};\n\nexport default ProfileInfo;\n","/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/ProfileLayout/editForm.jsx",["355","356","357","358"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/ProfileLayout/profileOrderCard.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/ProfileLayout/profileOrdersListModal.jsx",["359"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/skeleton/skeletonShopItem.jsx",[],[],{"ruleId":"360","severity":1,"message":"361","line":13,"column":8,"nodeType":"362","messageId":"363","endLine":13,"endColumn":19},{"ruleId":"360","severity":1,"message":"364","line":5,"column":8,"nodeType":"362","messageId":"363","endLine":5,"endColumn":15},{"ruleId":"360","severity":1,"message":"365","line":7,"column":9,"nodeType":"362","messageId":"363","endLine":7,"endColumn":20},{"ruleId":"360","severity":1,"message":"366","line":80,"column":5,"nodeType":"362","messageId":"363","endLine":80,"endColumn":16},{"ruleId":"360","severity":1,"message":"367","line":86,"column":7,"nodeType":"362","messageId":"363","endLine":86,"endColumn":26},{"ruleId":"360","severity":1,"message":"368","line":87,"column":7,"nodeType":"362","messageId":"363","endLine":87,"endColumn":23},{"ruleId":"360","severity":1,"message":"369","line":86,"column":17,"nodeType":"362","messageId":"363","endLine":86,"endColumn":24},{"ruleId":"360","sever