.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/shopPages/itemCard.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/api/index.js",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/shopPages/shopPage.jsx",["269"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/cartPage/cartItem.jsx",["270","271"],[],"import React, {useEffect, useState} from \"react\";\nimport { Link } from \"react-router-dom\";\nimport {useCart} from \"../../hooks/useCart\";\n\nconst CartItem = ( {data} ) => {\n    const { incrementItem, decrementItem, removeCartItem} = useCart()\n    const [exist, setExist] = useState(true)\n\n    const handleIncrement = () => {\n        data.counter++\n        incrementItem(data.item)\n    };\n\n    const handleDecrement = () => {\n        if (data.counter !== 1) {\n            data.counter--\n            decrementItem(data.item)\n        }\n    };\n\n    const  handleRemove = () => {\n        removeCartItem(data)\n        setExist(false)\n    }\n\n    return (\n        <>\n            { exist && (\n                <div className=\"cart__item\">\n                <img className=\"\" src={data.item.url}  width={150}/>\n\n                <div className=\"cart__item--title\">\n                <Link to={`/shop/${data.item._id}`}><h3>{data.item.name}</h3></Link>\n                <p>{data.item.category.name}</p>\n                </div>\n\n                <div className=\"cart__item--counter\">\n                <button onClick={handleDecrement}>-</button>\n                <p>{data.counter}</p>\n                <button onClick={handleIncrement}>+</button>\n                </div>\n\n                <div className=\"cart__item--price\">\n                <p>{data.item.price * data.counter} â‚½</p>\n                <button onClick={handleRemove}>remove</button>\n                </div>\n\n                </div>\n            )}\n        </>\n    );\n};\n\nexport default CartItem;\n","/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/loginForm.jsx",["272"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/shopPages/itemPage.jsx",["273","274","275","276"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/registerForm.jsx",["277","278","279"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/profileInfo.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/authPages/profileEdit.jsx",["280","281","282"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/components/common/navProfile.jsx",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/services/http.service.js",["283"],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/utils/validator.js",["284"],[],"export function validator(data, config) {\n    const errors = {};\n    function validate(validateMethod, data, config) {\n        let statusValidate;\n        switch (validateMethod) {\n        case \"isRequired\":\n            if (typeof data === \"boolean\") {\n                statusValidate = !data;\n            } else {\n                statusValidate = data.trim() === \"\";\n            }\n            break;\n        case \"isEmail\": {\n            const emailRegExp = /^\\S+@\\S+\\.\\S+$/g;\n            statusValidate = !emailRegExp.test(data);\n            break;\n        }\n        case \"isCapitalSymbol\": {\n            const capitalRegExp = /[A-Z]+/g;\n            statusValidate = !capitalRegExp.test(data);\n            break;\n        }\n        case \"isContainDigit\": {\n            const digitRegExp = /\\d+/g;\n            statusValidate = !digitRegExp.test(data);\n            break;\n        }\n        case \"min\": {\n            statusValidate = data.length < config.value;\n            break;\n        }\n        }\n        if (statusValidate) return config.message;\n    }\n\n    for (const fieldName in data) {\n        for (const validateMethod in config[fieldName]) {\n            const error = validate(validateMethod, data[fieldName], config[fieldName][validateMethod]);\n            if (error && !errors[fieldName]) {\n                errors[fieldName] = error;\n            }\n        }\n    }\n    return errors;\n}\n","/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/utils/paginate.js",[],[],"/Users/dariabrusnitsina/Yandex.Disk.localized/Programming/Projects/react-e-commerce/client/src/api/fake.api/items.api.js",["285"],[],"import { categoriesObject as categories } from \"./categories\";\n\nconst items = [\n    {\n        _id: \"1\",\n        name: \"Piala Constellation\",\n        url: \"https://sun9-69.userapi.com/90GKjpt3tR2qMGva0yNXJW-wV0ib-D0AAftqpA/5vpqj00OEqQ.jpg\",\n        category: categories.piala,\n        price: 1300,\n    },\n    {\n        _id: \"2\",\n        name: \"Piala Lune\",\n        url: \"https://sun9-17.userapi.com/M8iV0Dkabu3SrsTxDd0XekLlhBSkmSOwQszmdQ/9EAXo_G2N8U.jpg\",\n        category: categories.piala,\n        price: 1800,\n    },\n    {\n        _id: \"3\",\n        name: \"Piala Start\",\n        url: \"https://sun9-12.userapi.com/7xcNElj9EpJHq1aOvA_XDFXUZfHJF0_nPvBcMQ/e-tBD2K4WMo.jpg\",\n        category: categories.piala,\n        price: 1200,\n    },\n    {\n        _id: \"4\",\n        name: \"Piala Ait\",\n        url: \"https://sun9-2.userapi.com/aInXCWnLToCfu63neYxP-RGko1A-NJAdatiWwA/CtmmVjDz04Q.jpg\",\n        category: categories.piala